  //prettier
  "coc.preferences.formatOnSaveFiletypes": [
    "html",
    "css",
    "scss",
    "markdown",
    "go",
    "dart",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "sql"
  ],
  "sql.formatOptions": {
    "language": "postgresql",
    "uppercase": true,
    "indent": "    "
  },
  "sql.database": "postgresql",
  /* "prettier.statusItemText": "‚ìü ", */
