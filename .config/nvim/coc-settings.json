  //coc-list
  "list.insertMappings": {
    "<C-v>": "eval:@@",
    "<C-g>": "feedkeys:\\<ESC>",
    "<C-n>": "normal:j",
    "<C-p>": "normal:k"
  },
  //python
  "python.autoComplete.addBrackets": true,
  "python.jediEnabled": false,
  "python.pythonPath": "/home/dreamer/.pyenv/versions/nvim/bin/python",
  "python.formatting.provider": "yapf",
  //golang
  "go.goplsPath": "gopls",
  "go.goplsOptions": {
    "usePlaceholders": true,
    "completeUnimported": true,
    "deepCompletion": true
  },
  //Snippets
  /* "snippets.priority": 110, */
  /* "coc.preferences.snippetStatusText": "‚ìà ", */
  //CocList
  "list.source.grep.args": ["-S"],
  "list.source.files.excludePatterns": [
    "**/.git/**",
    "**/node_modules/**",
    "**/anroid/**",
    "**/ios/**",
    "**/build/**"
  ],
  /* "list.source.files.defaultOptions": ["--auto-preview"], */
